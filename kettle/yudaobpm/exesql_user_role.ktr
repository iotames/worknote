    <connection>BPMyudao</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>-- 为 tenant_id=1 且未分配角色的用户插入默认角色（bpm_admin）
INSERT INTO system_user_role (
    user_id, 
    role_id, 
    tenant_id, 
    creator, 
    create_time, 
    updater, 
    update_time, 
    deleted
)
SELECT 
    u.id AS user_id,
    (SELECT id FROM system_role WHERE code = 'bpm_user' AND tenant_id = 1 LIMIT 1) AS role_id,
    u.tenant_id,  -- 使用用户的租户ID
    '', 
    CURRENT_TIMESTAMP, 
    '', 
    CURRENT_TIMESTAMP, 
    b'0'          -- 未删除
FROM system_users u
LEFT JOIN system_user_role ur 
    ON u.id = ur.user_id 
    AND ur.deleted = b'0'   -- ✅ 关键：ON 子句确保仅关联有效角色
WHERE u.deleted = b'0'      -- 用户未删除
    AND u.tenant_id = 1     -- 用户租户ID=1
    AND ur.id IS NULL;      -- ✅ 用户未分配任何有效角色</sql>

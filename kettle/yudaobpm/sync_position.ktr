  <step>
    <name>找出用户和职位关系</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ERPodoo</connection>
    <sql>SELECT 
  -- 用户ID：管理员ID偏移+10000
  (m.s_manager_id + 10000) AS user_id,  
  -- 岗位ID：人才档案的岗位ID偏移+10000
  (a.s_talent_position_id + 10000) AS post_id,  
  -- 其他字段（固定值/转换逻辑）
  '' AS creator,  
  COALESCE(a.gmt_create, CURRENT_TIMESTAMP) AS create_time,  
  '' AS updater,  
  COALESCE(a.gmt_modified, a.gmt_create, CURRENT_TIMESTAMP) AS update_time,  
  CASE WHEN a.is_delete THEN b'1' ELSE b'0' END AS deleted,  
  1 AS tenant_id  
FROM ods.ods_php_erp_s_manager m
INNER JOIN ods.ods_php_erp_erp_talent_archives a 
  ON m.s_talent_id = a.s_talent_archives_id  -- 核心修正：按人才ID关联
WHERE
  a.s_talent_position_id is NOT NULL
  AND a.s_employee_num IS NOT NULL
  AND m.s_activate_flag = 1
  AND m.s_account &lt;&gt; '骆大春(旧)'
;</sql>

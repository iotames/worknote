    <connection>ERPodoo</connection>
    <sql>SELECT 
  (s_talent_position_id + 10000) AS id,  -- ID偏移+10000
  s_position AS name,                     -- 直接映射岗位名称
  COALESCE(NULLIF(s_en_position, ''), s_position) AS code,  -- 编码：英文名为空时用中文名
--  '' AS code,
  COALESCE(sort, 0) AS sort,               -- 排序值空值补0
  0 AS status,                            -- 状态固定为0（正常）
  '' AS remark,                           -- 备注置空
  '' AS creator,                          -- 创建者置空
  COALESCE(gmt_create, CURRENT_TIMESTAMP) AS create_time,  -- 创建时间空值用当前时间
  '' AS updater,                          -- 更新者置空
  COALESCE(gmt_modified, COALESCE(gmt_create, CURRENT_TIMESTAMP)) AS update_time, -- 更新时间优先用修改时间，次选创建时间
  CASE 
    WHEN is_delete = 1 THEN b'1' 
    ELSE b'0' 
  END AS deleted, 
  1 AS tenant_id 
FROM ods.ods_php_erp_erp_talent_position
WHERE is_delete = 0; </sql>

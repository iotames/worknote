  <step>
    <name>更新部门code</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ERPodoo</connection>
    <execute_each_row>Y</execute_each_row>
    <single_statement>N</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>WITH RECURSIVE cte AS (
    SELECT 
        id, 
        "parentId", 
        code, 
        id AS original_id,
        0 AS level,
        NULL::varchar(255) AS inherited_code
    FROM nocobase.departments
    WHERE code IS NULL

    UNION ALL

    SELECT 
        d.id, 
        d."parentId", 
        d.code, 
        c.original_id,
        c.level + 1,
        CASE 
            WHEN c.inherited_code IS NOT NULL THEN c.inherited_code
            ELSE d.code
        END
    FROM nocobase.departments d
    JOIN cte c ON d.id = c."parentId"
    WHERE c.inherited_code IS NULL
)
, cte2 AS (
    SELECT 
        original_id,
        inherited_code,
        level,
        ROW_NUMBER() OVER (
            PARTITION BY original_id 
            ORDER BY level
        ) AS rn
    FROM cte
    WHERE inherited_code IS NOT NULL
)
, cte3 AS (
    SELECT original_id, inherited_code
    FROM cte2
    WHERE rn = 1
)
UPDATE nocobase.departments
SET code = cte3.inherited_code
FROM cte3
WHERE 
    nocobase.departments.id = cte3.original_id
    AND nocobase.departments.code IS NULL;</sql>

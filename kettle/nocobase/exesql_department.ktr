    <quoteString>N</quoteString>
    <sql>UPDATE nocobase.departments A
SET "isLeaf" = TRUE,
    "updatedAt" = CURRENT_TIMESTAMP
WHERE NOT EXISTS (
    SELECT 1 FROM nocobase.departments C
    WHERE C."parentId" = A.id
);</sql>

  <step>
    <name>写入面料的物料颜色规格</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ERPodoo</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>Y</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>-- TODO:面料档
DO $$
DECLARE
    material_detail_record RECORD;
    new_template_id INTEGER;
BEGIN    
    -- 检查并添加 origin_id
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'oudu_base_material_details' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.oudu_base_material_details ADD COLUMN origin_id INTEGER';
    END IF;
    -- 使用游标逐条处理
    FOR material_detail_record IN 
        select  s_material_price_id as origin_id, 
                gmt_create as create_date,
                gmt_modified as write_date  , 
                s_width as name , 
                s_remark as memo ,  
                s_new_material_num as code ,
                --s_color_name ,
                cast(s_development_price as numeric) as price,
                b.id as color_id , 
                c.id as material_id
        from    ods.ods_php_erp_erp_material_price a inner join
                public.oudu_base_color b on b.name::json->>'zh_CN' = a.s_color_name inner join
                (
                    select  id , name::json->>'zh_CN' as name , weight_value , composition
                    from    public.oudu_base_material 
                ) as c on 
                   c.name = a.s_material_name and c.weight_value = a.s_weight and c.composition = a.s_fabric_composition
        where   s_supplier_name is not NULL and s_material_name is not NULL
                and s_width is not null 
				and s_new_material_num not like '-%'
                and not exists(
                      select  *
                      from    ods.ods_php_erp_erp_material_price d
                      where   d.s_material_name = a.s_material_name and 
                              d.s_weight = a.s_weight and 
                              d.s_fabric_composition = a.s_fabric_composition and 
                              d.s_color_name = a.s_color_name and
                              d.s_material_price_id > a.s_material_price_id
                )
                
    LOOP
        -- 插入模板表
        INSERT INTO public.oudu_properties_template (
            create_uid, write_uid, properties_definition, create_date, write_date
        )
        VALUES (
            1, 1,
            NULL,
            NOW(), NOW()
        )
        RETURNING id INTO new_template_id;
        
        -- 插入合作伙伴表
        INSERT INTO public.oudu_base_material_details(
            sequence, origin_id , name , memo , code , create_date , write_date , price , color_id , material_id , active , properties_tmpl_id
        )
        VALUES (
            10, material_detail_record.origin_id, material_detail_record.name, material_detail_record.memo , material_detail_record.code , 
            material_detail_record.create_date , material_detail_record.write_date , material_detail_record.price , material_detail_record.color_id , 
            material_detail_record.material_id , true , new_template_id
        );
        
        
    END LOOP;
END $$;</sql>

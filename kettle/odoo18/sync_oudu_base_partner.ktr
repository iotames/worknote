    <sql>-- TODO:供应商联系人
DO $$
DECLARE
    batch_size INTEGER := 100;
    counter INTEGER := 0;
    current_user_id INTEGER;
    supplier_record RECORD;
    new_template_id INTEGER;
BEGIN    
    -- 检查并添加 origin_id
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'oudu_base_partner' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.oudu_base_partner ADD COLUMN origin_id INTEGER';
    END IF;
    -- 使用游标逐条处理
    FOR supplier_record IN 
        SELECT ta.s_supplier_id, ta.s_supplier_num, COALESCE(ta.s_supplier_short_name,ta.s_supplier_name) as s_supplier_short_name , 
               COALESCE(ta.s_supplier_name,ta.s_supplier_short_name) as s_supplier_name, ta.s_address , ta.s_email,
               ta.s_remark as memo , tb.cpcs as desciption
        FROM    ods.ods_php_erp_erp_fabric_supplier ta 
                LEFT JOIN (
                            SELECT  
                                a.s_supplier_id,
                                COALESCE(b.s_zh_name,'') || COALESCE(c.name,'') || COALESCE(d.name,'') || COALESCE(e.name,'') as cpcs
                            FROM (
                                  SELECT 
                                      s_country_id,
                                      s_supplier_id,
                                      s_supplier_name,
                                      NULLIF(split_part(trim(both '[]' from s_country_id), ',', 1), '')::integer AS country_id_1,
                                      NULLIF(split_part(trim(both '[]' from s_country_id), ',', 2), '')::integer AS country_id_2,
                                      NULLIF(split_part(trim(both '[]' from s_country_id), ',', 3), '')::integer AS country_id_3,
                                      NULLIF(split_part(trim(both '[]' from s_country_id), ',', 4), '')::integer AS country_id_4
                                  FROM ods.ods_php_erp_erp_fabric_supplier
                                  WHERE s_country_id IS NOT NULL AND is_delete = 0
                                  ) as a 
                            LEFT JOIN ods.ods_php_erp_erp_country b ON b.s_country_id = a.country_id_1 
                            LEFT JOIN ods.ods_php_erp_area c ON c.id = a.country_id_2 
                            LEFT JOIN ods.ods_php_erp_area d ON d.id = a.country_id_3 
                            LEFT JOIN ods.ods_php_erp_area e ON e.id = a.country_id_4 
                          ) tb on tb.s_supplier_id = ta.s_supplier_id
        WHERE ta.is_delete = 0 
          and not exists(
              select  1 
              from    ods.ods_php_erp_erp_fabric_supplier as ta_1
              where   COALESCE(ta_1.s_supplier_name,ta_1.s_supplier_short_name) = COALESCE(ta.s_supplier_name,ta.s_supplier_short_name) and ta_1.s_supplier_id > ta.s_supplier_id
          )
          AND NOT EXISTS (
              SELECT 1 FROM public.oudu_base_partner tc 
              WHERE tc.origin_id = ta.s_supplier_id
          )
        ORDER BY ta.s_supplier_id
    LOOP
        -- 插入模板表
        INSERT INTO public.oudu_properties_template (
            create_uid, write_uid, properties_definition, create_date, write_date
        )
        VALUES (
            current_user_id, current_user_id,
            NULL,
            NOW(), NOW()
        )
        RETURNING id INTO new_template_id;
        
        -- 插入合作伙伴表
        INSERT INTO public.oudu_base_partner(
            sequence, origin_id, company_id, company_type, is_ok, code, 
            active, short_name, name, address, email, supplier_material, properties_tmpl_id , memo , "desc"
        )
        VALUES (
            10, supplier_record.s_supplier_id,
            (SELECT id FROM res_company ORDER BY id ASC LIMIT 1),
            'company', true, supplier_record.s_supplier_num,
            true, supplier_record.s_supplier_short_name,
            supplier_record.s_supplier_name, supplier_record.s_address,
            supplier_record.s_email, true, new_template_id , supplier_record.memo , supplier_record.desciption
        );
        
        counter := counter + 1;
        
        -- 每处理100条输出一次进度
        IF counter % batch_size = 0 THEN
            RAISE NOTICE '已处理 % 条记录', counter;
        END IF;
    END LOOP;
    
    RAISE NOTICE '处理完成，总共处理 % 条记录', counter;
END $$;</sql>

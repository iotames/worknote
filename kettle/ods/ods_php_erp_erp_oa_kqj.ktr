    <quoteString>N</quoteString>
    <sql>DO $$ 
BEGIN
    -- 若ods模式不存在则创建
    IF NOT EXISTS (SELECT FROM information_schema.schemata WHERE schema_name = 'ods') THEN
        EXECUTE 'CREATE SCHEMA ods';
    END IF;
    -- 检查ods模式下的表是否存在
    IF EXISTS (SELECT FROM information_schema.tables WHERE table_schema = 'ods' AND table_name = 'ods_php_erp_erp_oa_kqj') THEN
        -- EXECUTE 'DROP TABLE ods.ods_php_erp_erp_oa_kqj CASCADE';
        EXECUTE 'TRUNCATE TABLE ods.ods_php_erp_erp_oa_kqj CASCADE'; 
    ELSE
        -- 在ods模式下创建表结构
        CREATE TABLE ods.ods_php_erp_erp_oa_kqj (
            s_id SERIAL PRIMARY KEY,
            id INTEGER,
            user_id INTEGER,
            ext_id INTEGER,
            terminal_id VARCHAR(255),
            check_type VARCHAR(50),
            check_time TIMESTAMP,
            check_data TEXT,
            s_created_time TIMESTAMP,
            is_write SMALLINT DEFAULT 0
        );

        -- 表注释
        COMMENT ON TABLE ods.ods_php_erp_erp_oa_kqj IS 'OA考勤-考勤机数据';
        
        -- 列注释
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.s_id IS '自增主键';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.id IS '打卡记录ID';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.user_id IS '打卡人员ID(考勤机里的)';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.ext_id IS '打卡人员用户ID';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.terminal_id IS '打卡数据的终端ID。包括：设备打卡: 设备的SN；手机打卡(包括 wifi/gps/外勤打卡方式)：打卡手机的唯一设备id；补卡: 固定值 "apply"';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.check_type IS '打卡方式。包括：fa: 人脸；fp: 指纹；pass: 密码；card: 刷卡；app_scan: APP扫码；gps: gps位置打卡；wifi: wifi打卡；out_work: 外勤打卡；reissue: 补签申请；flexible: 自动补的灵活卡';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.check_time IS '打卡时间';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.check_data IS '打卡补充数据，不同的打卡类型则返回不同的数据';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.s_created_time IS '创建时间';
        COMMENT ON COLUMN ods.ods_php_erp_erp_oa_kqj.is_write IS '0:未写入，1：写入';

        -- 创建索引
        CREATE INDEX idx_ods_php_erp_erp_oa_kqj_is_write ON ods.ods_php_erp_erp_oa_kqj(is_write);
        CREATE INDEX idx_ods_php_erp_erp_oa_kqj_s_id ON ods.ods_php_erp_erp_oa_kqj(s_id);

    END IF;
END 
$$;</sql>

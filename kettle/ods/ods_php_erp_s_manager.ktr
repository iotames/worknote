  <step>
    <name>清空用户基础表</name>
    <type>ExecSQL</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>ERPodoo</connection>
    <execute_each_row>N</execute_each_row>
    <single_statement>Y</single_statement>
    <replace_variables>N</replace_variables>
    <quoteString>N</quoteString>
    <sql>DO $$
BEGIN
    -- 若ods模式不存在则创建
    IF NOT EXISTS (SELECT FROM information_schema.schemata WHERE schema_name = 'ods') THEN
        EXECUTE 'CREATE SCHEMA ods';
    END IF;
    -- 检查ods模式下的表是否存在
    IF EXISTS (SELECT FROM information_schema.tables WHERE table_schema = 'ods' AND table_name = 'ods_php_erp_s_manager') THEN
        -- EXECUTE 'DROP TABLE ods.ods_php_erp_s_manager CASCADE';
        EXECUTE 'TRUNCATE TABLE ods.ods_php_erp_s_manager CASCADE'; 
    ELSE
        -- 创建表结构
        CREATE TABLE ods.ods_php_erp_s_manager (
            s_manager_id SERIAL PRIMARY KEY,
            s_creater INT,
            s_account VARCHAR(64),
            s_account_pinyin VARCHAR(255),
            s_account_pinyin_first VARCHAR(255),
            s_password VARCHAR(64),
            s_cellphone VARCHAR(20),
            s_email VARCHAR(64),
            s_activate_flag INT CHECK (s_activate_flag IN (0,1)),
            s_unionid VARCHAR(255),
            s_openid VARCHAR(255),
            dd_depart_ids TEXT,
            s_created_time TIMESTAMP,
            s_updated_time TIMESTAMP,
            s_deleted_time TIMESTAMP,
            s_lead_flag SMALLINT,
            s_talent_id INT,
            s_mail_nickname VARCHAR(255),
            s_sys_language VARCHAR(10) DEFAULT 'zh',
            s_time_zone INT
        );

        -- 添加注释
        COMMENT ON TABLE ods.ods_php_erp_s_manager IS '后台管理员表';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_creater IS '用户创建者';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_account IS '账户';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_account_pinyin IS '姓名的拼音';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_account_pinyin_first IS '拼音首字母';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_password IS '密码';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_cellphone IS '手机号';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_email IS '电子邮箱';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_activate_flag IS '是否启用(1.启用 0禁用）';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_unionid IS '微信开放平台unionid';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_openid IS '微信小程序openid';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.dd_depart_ids IS '钉钉部门ID列表';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_created_time IS '创建时间';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_updated_time IS '更新时间';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_deleted_time IS '逻辑删除时间';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_lead_flag IS '最高领导者标识';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_talent_id IS '人事档案ID';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_mail_nickname IS '邮箱显示昵称';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_sys_language IS '系统语言';
        COMMENT ON COLUMN ods.ods_php_erp_s_manager.s_time_zone IS '时区ID';
    END IF;
END 
$$;</sql>

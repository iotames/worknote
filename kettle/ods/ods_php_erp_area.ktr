    <quoteString>N</quoteString>
    <sql>DO $$
BEGIN
    -- 若ods模式不存在则创建
    IF NOT EXISTS (SELECT FROM information_schema.schemata WHERE schema_name = 'ods') THEN
        EXECUTE 'CREATE SCHEMA ods';
    END IF;
    -- 检查ods模式下的表是否存在 
    IF EXISTS (SELECT FROM information_schema.tables WHERE table_schema = 'ods' AND table_name = 'ods_php_erp_area') THEN
        EXECUTE 'TRUNCATE TABLE ods.ods_php_erp_area CASCADE'; 
    ELSE
        -- 创建表结构
       CREATE TABLE ods.ods_php_erp_area (
										  id serial PRIMARY KEY,
										  pid integer NOT NULL DEFAULT 0,
										  citycode varchar(4),
										  adcode varchar(6),
										  name varchar(100),
										  detail_name varchar(200),
										  lng decimal(10,6),
										  lat decimal(10,6),
										  level varchar(10),
										  initials varchar(2),
										  pinyin varchar(200)
										);

		COMMENT ON TABLE ods.ods_php_erp_area IS '地区表';
		COMMENT ON COLUMN ods.ods_php_erp_area.pid IS '上级城市';
		COMMENT ON COLUMN ods.ods_php_erp_area.citycode IS '城市编码';
		COMMENT ON COLUMN ods.ods_php_erp_area.adcode IS '区域编码';
		COMMENT ON COLUMN ods.ods_php_erp_area.name IS '地区名称';
		COMMENT ON COLUMN ods.ods_php_erp_area.detail_name IS '详细名称';
		COMMENT ON COLUMN ods.ods_php_erp_area.lng IS '中心点:经度';
		COMMENT ON COLUMN ods.ods_php_erp_area.lat IS '中心点:纬度';
		COMMENT ON COLUMN ods.ods_php_erp_area.level IS '城市级别';
		COMMENT ON COLUMN ods.ods_php_erp_area.initials IS '首字母';
		COMMENT ON COLUMN ods.ods_php_erp_area.pinyin IS '拼音';

    END IF;
END 
$$;</sql>

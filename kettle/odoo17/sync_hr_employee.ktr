    <sql>       
DO $$
BEGIN
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'hr_employee' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.hr_employee ADD COLUMN origin_id INTEGER';
    END IF;
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'resource_resource' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.resource_resource ADD COLUMN origin_id INTEGER';
    END IF;
END $$;

BEGIN;

WITH result_data AS (
    -- 查询结果
    SELECT  
        a.s_manager_id as origin_id, 
        s_account as name, 
        s_cellphone as work_phone, 
        a.s_email as work_email, 
        'employee' as employee_type, 
        case s_activate_flag when 1 then true when 0 then false end as active, 
        s_lead_flag, 
        s_subcompany_id, 
        s_employee_num as barcode, 
        c.id as department_id, 
        d.id as job_id, 
        (select id from res_company WHERE name like '泉州森地客') as company_id
    FROM ods.ods_php_erp_s_manager a 
    INNER join ods.ods_php_erp_erp_talent_archives b on b.s_talent_archives_id = a.s_talent_id 
    left join public.hr_department c on c.origin_id = cast(a.dd_depart_ids as integer) 
    left join public.hr_job d on d.origin_id = b.s_talent_position_id
    --limit 1
),

-- 先写入resource_resource
resource_insert AS (
    INSERT INTO resource_resource (
        name, resource_type, active, company_id, origin_id, tz, time_efficiency,
        create_date, create_uid, write_date, write_uid
    )
    SELECT 
        rd.name, 'user', rd.active, rd.company_id, rd.origin_id, 'Asia/Shanghai', 100.0 ,
        now(), 1, now(), 1
    FROM result_data rd 
    WHERE NOT EXISTS ( 
        SELECT 1
        FROM resource_resource
        WHERE origin_id = rd.origin_id AND resource_type = 'user'
    )
),

-- 插入employee表
employee_insert AS (
    INSERT INTO hr_employee (
        name, resource_id, company_id, work_phone, work_email,
        employee_type, active, department_id, job_id, barcode, origin_id ,
        create_date, create_uid, write_date, write_uid
    )
    SELECT 
        sd.name, rr.id, sd.company_id, sd.work_phone, sd.work_email,
        sd.employee_type, sd.active, sd.department_id, sd.job_id, sd.barcode, sd.origin_id,
        now(), 1, now(), 1
    FROM result_data sd 
    INNER JOIN resource_resource rr ON rr.origin_id = sd.origin_id
    WHERE NOT EXISTS ( 
        SELECT 1
        FROM hr_employee
        WHERE origin_id = sd.origin_id AND employee_type = 'employee'
    )
),

-- 更新resource_resource
resource_update AS (
    UPDATE resource_resource 
    SET name = rt.name, 
        active = rt.active
    FROM result_data rt 
    WHERE rt.origin_id = resource_resource.origin_id
    RETURNING resource_resource.id 
    
),  

-- 更新hr_employee
employee_update AS (
    UPDATE hr_employee 
    SET name = rt.name, 
        active = rt.active, 
        department_id = rt.department_id, 
        job_id = rt.job_id 
    FROM result_data rt 
    WHERE rt.origin_id = hr_employee.origin_id AND hr_employee.employee_type = 'employee'
    RETURNING hr_employee.id
),

-- 更新resource_resource表中origin_id为NULL的记录
update_null_resource AS (
    UPDATE resource_resource
    SET active = false,
        write_date = now(),
        write_uid = 1
    WHERE origin_id IS NULL
    RETURNING id
),

-- 更新hr_employee表中origin_id为NULL的记录
update_null_employee AS (
    UPDATE hr_employee
    SET active = false,
        write_date = now(),
        write_uid = 1
    WHERE origin_id IS NULL AND employee_type = 'employee'
    RETURNING id
)

SELECT 'Data inserted and updated successfully' AS result;

COMMIT;</sql>

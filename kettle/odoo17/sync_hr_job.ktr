    <sql>-- 检查并添加 origin_id
DO $$
BEGIN
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'hr_job' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.hr_job ADD COLUMN origin_id INTEGER';
    END IF;
END $$;

-- 不存在于ods 岗位中的记录归档掉
UPDATE  public.hr_job  as ta
SET     active = false
WHERE   not EXISTS (select s_talent_position_id from ods.ods_php_erp_erp_talent_position as tb  where  tb.s_talent_position_id = ta.origin_id );

-- 插入不存在于odoo_erp中的ods中的记录

INSERT INTO hr_job( origin_id , name , active, company_id )
select  ods_job.s_talent_position_id as origin_id , cast(jsonb_build_object('zh_CN', ods_job.s_position, 'en_US', COALESCE(ods_job.s_en_position,ods_job.s_position))::TEXT AS JSONB)  AS name,   
        case ods_job.is_delete when 1 then false when 0 then true end as active, (select  id  from  res_company		WHERE   name like '泉州森地客' ) as company_id
from    ods.ods_php_erp_erp_talent_position as ods_job
WHERE   not EXISTS (
                      select  1  
                      from    public.hr_job as hr_job
                      where   hr_job.origin_id = ods_job.s_talent_position_id
                    );

-- ods name 和 is_delete 与odoo_erp 不一致
UPDATE  public.hr_job
SET     name = t.name , active = t.active
from    (
          select  ta.id , tb.name , tb.active
          FROM    public.hr_job as ta inner join 
                  (
                    select  s_talent_position_id , 
                            cast(jsonb_build_object('zh_CN', s_position, 'en_US', COALESCE(s_en_position,s_position))::TEXT AS JSONB)  AS name ,
                            case is_delete when 1 then false when 0 then true end as active
                    from    ods.ods_php_erp_erp_talent_position
                  ) as tb on tb.s_talent_position_id = ta.origin_id
          where   tb.name &lt;&gt; ta.name or tb.active &lt;&gt; ta.active
        ) t ;</sql>

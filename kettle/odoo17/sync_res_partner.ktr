    <sql>-- TODO:供应商联系人
-- 检查并添加 origin_id
DO $$
BEGIN
    IF NOT EXISTS (
        SELECT 1 FROM information_schema.columns 
        WHERE table_name = 'res_partner' 
        AND column_name = 'origin_id'
    ) THEN
        EXECUTE 'ALTER TABLE public.res_partner ADD COLUMN origin_id INTEGER';
    END IF;
	
END $$;

INSERT INTO public.res_partner (supplier_rank,
    origin_id,
    customer_code,
    name,
    complete_name,
    tz,
    lang,
    type,
    supplier_type,
    customer_short_name,
    street,
    employee_number,
    production_staff,
    technical_staff,
    email,
    email_normalized,
    active,
    is_company,
    partner_share,
    state,
    status,
    score,
    comment,
    country_id,
    state_id,
    city
)
SELECT 1,
    ta.s_supplier_id as origin_id,
    s_supplier_num AS customer_code,
    s_supplier_name AS name,
    s_supplier_name as complete_name,
    'Asia/Shanghai' as tz,
    'zh_CN' as lang,
    'contact' as type,
    (
        SELECT
            b.id
        FROM
            santic_basedata_dict a
            INNER JOIN santic_basedata_dict_detail b ON b.dict_id = a.id
        WHERE
            a.code = 'supplier_type'
            AND b.code = 'material'
    ) AS supplier_type,
    s_supplier_short_name as customer_short_name,
    s_address as street,
    s_total_person as employee_number,
    s_workshop_person as production_staff,
    s_technical_person as technical_staff,
    s_email as email,
    s_email as email_normalized,
    true as active,
    true as is_company,
    true as partner_share,
    'active' as state,
    'draft' as status,
    cast(s_score as integer)as score,
    s_remark as comment,
    tb.country_id as country_id,
    tb.state_id as state_id,
    tb.city as city
FROM
    ods.ods_php_erp_erp_fabric_supplier ta 
LEFT JOIN 
    (
        SELECT  
            a.s_supplier_id,
            f.id as country_id,
            g.id as state_id,
            d.name as city
        FROM    
            (
                SELECT 
                    s_country_id,
                    s_supplier_id,
                    s_supplier_name,
                    NULLIF(split_part(trim(both '[]' from s_country_id), ',', 1), '')::integer AS country_id_1,
                    NULLIF(split_part(trim(both '[]' from s_country_id), ',', 2), '')::integer AS country_id_2,
                    NULLIF(split_part(trim(both '[]' from s_country_id), ',', 3), '')::integer AS country_id_3,
                    NULLIF(split_part(trim(both '[]' from s_country_id), ',', 4), '')::integer AS country_id_4
                FROM 
                    ods.ods_php_erp_erp_fabric_supplier
                WHERE 
                    s_country_id IS NOT NULL 
                    AND is_delete = 0
            ) as a 
        LEFT JOIN 
            ods.ods_php_erp_erp_country b ON b.s_country_id = a.country_id_1 
        LEFT JOIN 
            ods.ods_php_erp_area c ON c.id = country_id_2 
        LEFT JOIN 
            ods.ods_php_erp_area d ON d.id = country_id_3 
        LEFT JOIN
            ods.ods_php_erp_area e ON e.id = country_id_4 
        LEFT JOIN
            public.res_country f ON (f.name::json->>'zh_CN') = b.s_zh_name 
        LEFT JOIN
            public.res_country_state g ON g.name = c.name
    ) tb ON tb.s_supplier_id = ta.s_supplier_id
WHERE   
    is_delete = 0 
    AND s_email IS NOT NULL
	AND NOT EXISTS( SELECT 1 FROM res_partner tc where tc.origin_id = ta.s_supplier_id)
ORDER BY 
    s_email ASC;

-- 写入res.partner的所属类型

INSERT INTO public.partner_property_rel(partner_id , property_id)
select  rp.id as partner_id , sdd.id as property_id
from    public.res_partner as rp CROSS join 
        (
          select  id 
          from    santic_data_dict 
          where   type = 'customer_property' and code = 'customer_property.supplier'
        ) as sdd 
where 	rp.origin_id is not null and not EXISTS( select 1 from	partner_property_rel  ppr 	where ppr.partner_id = rp.id and sdd.id = ppr.property_id );

</sql>
